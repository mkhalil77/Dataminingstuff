import operator

dict = {'khalil': 23, 'Chrstian': 27, 'Maria': 21}
def InsertToDec (dict,Function, Quality):
    dict[Function]=Quality
    sorted_x = sorted(dict.items(), key=operator.itemgetter(1))
    return sorted_x


def MaxValue(dict):
    maxi=max(dict.iteritems(), key=operator.itemgetter(1))[0]
    return maxi





print InsertToDec(dict,"Tot",10)

print MaxValue(dict)




